import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int i,j,k,n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for(i=1;i<=n;i++)
        {
            for(k=n-i;k>=1;k--)
                System.out.print(" ");
            for(j=0;j<i;j++)
                System.out.print("#");
            System.out.println("");
        }
        
    }
}
